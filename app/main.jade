      // Media
      section.media-section
        mixin media-item(image,media_date,media_head,delay)
          .col.l4.m12.s12
            a.media-item.animated(href="#",data-animation="InDown",data-animation-delay="#{delay}")
              img(src="assets/images/#{image}.jpg", alt="")
              span.media-item-caption
                span.media-date= media_date
                span.media-head= media_head
                span.link-more
                  span.icon-ico04 
                  span.lnk-txt Read more
        .container
          .heading-section Media
          .row
            +media-item('media1','25.10.2016','Charity of the Month','100')
            +media-item('media2','16.10.2016','Autism Speaks','200')
